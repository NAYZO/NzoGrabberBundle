    "description": "The NzoGrabberBundle is a Symfony Bundle used to Crawl and to Grab all types of links and Tags for (img, js, css) from any website",
    "keywords": ["DomCrawler", "Link", "url", "Grab", "scan", "search", "crawl"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ala Eddine Khefifi",
            "email": "alakhefifi@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "symfony/framework-bundle": "^3.0|^4.0",
        "fabpot/goutte": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": { "Nzo\\GrabberBundle\\": "" }
    }
}
